{
    "name": "10up/wp-compat-validation-tool",
    "description": "Perform PHP and WP version compatibility checks in your plugin.",
    "authors": [
        {
          "name": "10up",
          "email": "opensource@10up.com",
          "homepage": "https://10up.com",
          "role": "Developer"
        }
    ],
    "homepage": "https://github.com/10up/wp-compat-validation-tool",
    "readme": "https://github.com/10up/wp-compat-validation-tool#readme",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WP_Compat_Validation_Tool\\": "src/"
        }
    },
    "require": {
        "composer/installers": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-name": "compat-lib/wp-compat-validation-tool"
    },
    "type": "wordpress-plugin"
}
